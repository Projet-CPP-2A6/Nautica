   <property name="styleSheet">
    <string notr="true">  #centralwidget {
    margin: 0;
    padding: 0;
    height: 100vh;
    position: relative;
    background-color: #87CEEB; /* Light blue color for the ocean */
  }

  #centralwidget::before,
  #centralwidget::after {
    content: &quot;&quot;;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height:300px; /* Adjust the height of the waves */
    background: linear-gradient(to bottom, transparent 40%, rgba(255, 255, 255, 0.4) 60%, rgba(255, 255, 255, 0.8) 80%, rgba(255, 255, 255, 0.9) 100%);
    border-radius: 50%;
    transform-origin: top;
    animation: wave 10s ease-in-out infinite alternate;
  }

  #centralwidget::before {
    animation-delay: -2s;
  }

  #centralwidget::after {
    animation-delay: -4s;
  }

  @keyframes wave {
    0% {
      transform: translateX(-25%) scaleY(1.1);
    }
    50% {
      transform: translateX(25%) scaleY(0.9);
    }
    100% {
      transform: translateX(0) scaleY(1.1);
    }
  }
</string>
